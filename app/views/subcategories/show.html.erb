    <div class="col-lg-12">
        <h1 class="page-header">Thumbnail Gallery</h1>
    </div>
    <% @product.each do |product| %>

    <div class="col-lg-3 col-md-4 col-xs-6 thumb">
        <a class="thumbnail" href="#">
            <img class="img-responsive" alt=""<%= image_tag product.picture_url, size: "25" %>
        </a>
    </div>
    <% end %>